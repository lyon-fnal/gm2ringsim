process_name:myProcessName

source: {
  module_type: EmptyEvent
  maxEvents:  1
}

services: {

  message : {
     debugModules : ["*"]
     suppressInfo : []
     
     destinations : {
       LogToConsole : {
         type : "cout"
         threshold : "INFO"
         
         categories : {
           default : { limit : 50 }
         }
       }
     }
  }

  user: {
    DetectorHolder: {}
    ActionHolder: {}
    Material: {}
    RandomNumberGenerator: {}
    PhysicsListHolder: {}
    Geometry: {
          SampleGeometry: {
         sampleInt : 97
         sampleDouble : 97.333
      }         
         WedgeGeometry :{
           length : 14
           angle : 3.1415
         }
      
    }

    // Detector(s) for the simulation

    // Action(s) for the simulation
